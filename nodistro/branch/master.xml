  <!-- STM32 layer -->
  <!-- <project remote="stm32mp" revision="master" name="meta-st-stm32mp" path="layers/meta-st-stm32mp" /> -->

  <!-- Atmel layer -->
  <project remote="tegra" revision="master" name="meta-tegra" path="layers/meta-tegra" />

